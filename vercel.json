{
  "version": 2,
  "name": "novartis-nhs-project",
  "builds": [
    {
      "src": "novartis-nhs-project/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "novartis-nhs-project/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/novartis-nhs-project/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/novartis-nhs-project/index.html"
    }
  ]
}
